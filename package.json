{
  "name": "foodshare",
  "version": "3.0.2",
  "private": true,
  "engines": {
    "node": ">=20.0.0 || >=24.0.0",
    "bun": ">=1.1.0"
  },
  "scripts": {
    "dev": "bun --bun next dev --turbopack -p 3000",
    "build": "bun --bun next build",
    "build:analyze": "ANALYZE=true bun --bun next build",
    "start": "bun --bun next start",
    "lint": "bunx eslint",
    "lint:fix": "bunx eslint --fix",
    "type-check": "bunx tsc --noEmit",
    "test": "bun test src/__tests__/actions/admin.test.ts src/__tests__/actions/auth.test.ts src/__tests__/actions/email.test.ts src/__tests__/actions/newsletter.test.ts src/__tests__/actions/products.test.ts src/__tests__/lib/ src/store/ src/utils/ src/api/ src/lib/logger/ src/lib/testing/ && bun test src/__tests__/actions/chat.test.ts && bun test src/__tests__/integration/",
    "test:watch": "bun test --watch src/",
    "test:coverage": "bun test --coverage src/__tests__/actions/admin.test.ts src/__tests__/actions/auth.test.ts src/__tests__/actions/email.test.ts src/__tests__/actions/newsletter.test.ts src/__tests__/actions/products.test.ts src/__tests__/lib/ src/store/ src/utils/ src/api/ src/lib/logger/ src/lib/testing/ && bun test --coverage src/__tests__/actions/chat.test.ts && bun test --coverage src/__tests__/integration/",
    "test:ci": "bun test --ci --coverage src/__tests__/actions/admin.test.ts src/__tests__/actions/auth.test.ts src/__tests__/actions/email.test.ts src/__tests__/actions/newsletter.test.ts src/__tests__/actions/products.test.ts src/__tests__/lib/ src/store/ src/utils/ src/api/ src/lib/logger/ src/lib/testing/ && bun test --ci --coverage src/__tests__/actions/chat.test.ts && bun test --ci --coverage src/__tests__/integration/",
    "e2e": "bunx playwright test",
    "e2e:ui": "bunx playwright test --ui",
    "e2e:headed": "bunx playwright test --headed",
    "e2e:debug": "bunx playwright test --debug",
    "e2e:report": "bunx playwright show-report",
    "test:build": "bun run type-check && bun run lint && bun run build",
    "clean": "rm -rf .next out node_modules/.cache .turbo",
    "postinstall": "bun run patch-package",
    "import:foodbanks": "bun run scripts/foodbank-import/index.ts",
    "email:dev": "bunx email dev -p 3001 --dir src/emails",
    "email:export": "bunx email export --outDir src/emails/templates/exported",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f foodshare-web"
  },
